{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"path": "$['Column_1']"
								}
							},
							{
								"source": {
									"name": "Age",
									"type": "Int64"
								},
								"sink": {
									"path": "$['Column_2']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SampleDate",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}